{
    "extends": "airbnb-base",
    "rules": {
        "no-console": "off",
        "linebreak-style": 0,
        "prefer-template": "error",
        "no-return-assign": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off",
        "no-alert": "off",
        "func-names": "off",
        "no-shadow": "off",
        "no-unused-expressions": "off",
        "no-unused-vars":"off",
        "no-undef":"off",
        "eol-last": "off"
    }
}